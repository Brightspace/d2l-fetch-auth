    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "rollup": "^0.67.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "sinon": "^14"
  },
  "dependencies": {
    "frau-jwt": "^2.0.3"
  },
  "files": [
    "dist/",
    "es6/",
    "src/",
    "d2l-fetch-auth-framed.js",
