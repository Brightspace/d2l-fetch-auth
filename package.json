{
  "description": "Provides a middleware function for wrapping a window.Request object with d2l authentication for use with d2l-fetch",
  "keywords": [
    "fetch",
    "middleware",
    "auth"
  ],
  "name": "d2l-fetch-auth",
  "main": "index.js",
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rollup -c rollup/rollup.config.js",
    "pretest": "npm run build",
    "test:lint:js": "eslint src/ test/ --ext .js",
    "test:lint": "npm run test:lint:wc && npm run test:lint:js",
    "test": "npm run test:lint && npm run test:polymer:local",
    "test:polymer:local": "cross-env LAUNCHPAD_BROWSERS=firefox polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local",
    "publish-location": "node publishLocation",
    "prepublish-release": "npm run build",
    "publish-release": "frau-publisher && npm run publish-location"
  },
  "config": {
    "frauPublisher": {
      "files": "./dist/**",
      "moduleType": "lib",
      "targetDirectory": "d2lfetch-auth",
      "creds": {
        "key": "AKIAI2Z7AUDCZQ4V4KZA",
        "secretVar": "CDN_SECRET"
      },
      "devTagVar": "TRAVIS_COMMIT",
      "versionVar": "TRAVIS_TAG"
    }
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-brightspace": "^0.2.4",
    "eslint-plugin-html": "^2.0.1",
    "frau-publisher": "^2.6.2",
    "polymer-cli": "^1.9.1",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "wct-browser-legacy": "^1.0.1"
  },
  "dependencies": {
    "frau-jwt": "^2.0.0"
  },
  "version": "",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  }
}
