    "fetch",
    "middleware",
    "auth"
  ],
  "name": "d2l-fetch-auth",
  "main": "index.js",
  "module": "index.es6.js",
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rollup -c rollup/rollup.config.js",
    "lint": "eslint . --ext .js",
    "prepare": "npm run build",
    "pretest": "rollup -c rollup/rollup.test.config.js",
    "test": "npm run lint &&  npm run test:headless",
    "test:headless": "web-test-runner --files ./test/**/*.test.js --node-resolve",
    "test:headless:watch": "web-test-runner --files ./test/**/*.test.js --node-resolve --watch"
